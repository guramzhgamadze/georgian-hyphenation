<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xsi:type="TaskPaneApp">
  <Id>4a32aa67-73bd-42ba-8499-8e18f61cb54c</Id>
  <Version>2.2.6</Version>
  <ProviderName>Guram Zhgamadze</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  
  <DisplayName DefaultValue="Georgian Hyphenation">
    <Override Locale="ka-GE" Value="ქართული დამარცვლა"/>
  </DisplayName>
  
  <Description DefaultValue="Automatic hyphenation for Georgian text in Microsoft Word. Perfect for justified alignment and professional document formatting.">
    <Override Locale="ka-GE" Value="ავტომატური დამარცვლა ქართული ტექსტისთვის Microsoft Word-ში. იდეალურია გასწორებული განლაგებისა და პროფესიული დოკუმენტების ფორმატირებისთვის."/>
  </Description>
  
  <IconUrl DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/guramzhgamadze/georgian-hyphenation"/>
  
  <AppDomains>
    <AppDomain>https://cdn.jsdelivr.net</AppDomain>
    <AppDomain>https://guramzhgamadze.github.io</AppDomain>
  </AppDomains>
  
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  
  <DefaultSettings>
    <SourceLocation DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/src/taskpane/taskpane.html"/>
  </DefaultSettings>
  
  <Permissions>ReadWriteDocument</Permissions>
  
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Title"/>
                    <Description resid="TaskpaneButton.Tip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/assets/icon-80.png"/>
        <bt:Image id="Icon.128x128" DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/assets/icon-128.png"/>
        <bt:Image id="Icon.256x256" DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/assets/icon-256.png"/>
      </bt:Images>
      
      <bt:Urls>
        <bt:Url id="Taskpane.Url" DefaultValue="https://guramzhgamadze.github.io/georgian-hyphenation/word-addin/src/taskpane/taskpane.html"/>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/guramzhgamadze/georgian-hyphenation"/>
      </bt:Urls>
      
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Georgian Hyphenation"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Georgian"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Hyphenate"/>
        <bt:String id="TaskpaneButton.Title" DefaultValue="Georgian Hyphenation"/>
      </bt:ShortStrings>
      
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Add automatic hyphenation to Georgian text for professional document formatting."/>
        <bt:String id="TaskpaneButton.Tip" DefaultValue="Click to open the Georgian Hyphenation panel. Automatically adds soft hyphens to Georgian text for perfect justified alignment."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>